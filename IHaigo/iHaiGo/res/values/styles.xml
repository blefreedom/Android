<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 定义tabhost -->
    <style name="main_tab_bottom">
        <item name="android:textSize">@dimen/bottom_tab_font_size</item>
        <item name="android:textColor">@drawable/tab_color_selector</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">#ffffff</item>
        <item name="android:paddingTop">@dimen/bottom_tab_padding_up</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:drawablePadding">@dimen/bottom_tab_padding_drawable</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <!-- 定义分类搜索页面一级菜单 -->
    <style name="dot_style">
        <item name="android:layout_width">5dip</item>
        <item name="android:layout_height">5dip</item>
        <item name="android:background">@drawable/rec_dot_normal</item>
        <item name="android:layout_marginLeft">1.5dip</item>
        <item name="android:layout_marginRight">1.5dip</item>
    </style>

    <style name="customProgressBar" parent="@android:style/Widget.ProgressBar.Small">
        <item name="android:indeterminateDrawable">@drawable/custom_progress_bar</item>
        <item name="android:indeterminateOnly">true</item>
        <item name="android:indeterminateDuration">200</item>
        <item name="android:indeterminateBehavior">repeat</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
    </style>
    <!-- 分类搜索主页 -->
    <style name="sortsearch_textview">
        <item name="android:textSize">18sp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:drawableLeft">@drawable/sortsearch_searchimg</item>
        <item name="android:hint">@string/sortsearch_search_hint</item>
        <item name="android:paddingLeft">30dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:background">@drawable/sortsearch__searchview_shape</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:textColor">@color/sortsearchfont</item>
    </style>
    <!-- 搜索后列表-商品名称 -->
    <style name="goodsName">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <!-- 搜索后列表-实际价格 -->
    <style name="actualPrice">
        <item name="android:textSize">15sp</item>
    </style>
    <!-- 搜索后列表-原来价格 -->
    <style name="originalPrice">
        <item name="android:textSize">12sp</item>
    </style>
    <!-- 加入购物车样式 -->
    <style name="addAdvDetails">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/add_advDetails</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:background">@drawable/shopping_btn_shape</item>
        <item name="android:gravity">center</item>
    </style>
    <!-- 广告列表-美元原来价格 -->
    <style name="adv_originalPrice">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/adv_originalPrice</item>
    </style>
    <!-- 广告列表-美元实际价格 -->
